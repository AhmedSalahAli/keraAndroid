<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="90"
                android:startColor="#F379C6"
                android:endColor="#0093DE"
                android:type="linear" />
            <size android:width="24dp"
                android:height="24dp"/>
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle" >
            <solid android:color="#E9EFF7" />
        </shape>
    </item>

</layer-list>