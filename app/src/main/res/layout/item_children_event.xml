<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="model"
            type="com.app.kera.events.model.EventDetailsResponseModel.DataBean.StudentData" />
    </data>
<FrameLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="11dp">
    
    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="@dimen/_35sdp"
        android:layout_height="@dimen/_35sdp"
        app:civ_border_width="1dp"
        app:civ_border_color="#ECC89C"
        android:layout_marginBottom="10dp"
        app:image="@{model.student.profileImage}"
        />
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_accept"
        android:layout_width="@dimen/_24sdp"
        android:layout_height="@dimen/_24sdp"
        android:src="@drawable/ic_student_uncheked"
        android:layout_gravity="bottom|center"
        />

    
</FrameLayout>
</layout>
